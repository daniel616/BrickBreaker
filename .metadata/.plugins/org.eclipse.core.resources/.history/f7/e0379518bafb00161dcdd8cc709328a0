package commands.TurtleCommands;

import java.util.List;

import application.Turtle;
import application.TurtleState;
import exceptions.StateNotFoundException;

public class Home extends TurtleCommands {
	
	public static final int VARIABLES = 0;

	public Home(List<Double> args) {
		super(args);
	}

	public double execute(Turtle t) throws StateNotFoundException{
		TurtleState lastState = t.getFinalTurtleState();
		t.addTurtleState(0, 0, lastState.getHeading(), lastState.penDown(), lastState.getColor(), lastState.turtleShowing());
		return Math.sqrt(Math.pow(lastState.getX(), 2)+Math.pow(lastState.getY(),2));
		
	}
}
