package application;

import javafx.scene.image.ImageView;

public class PowerUp {
	//Stores information concerning which kind of PowerUp each object is.
	
	private int version;
	private ImageView sprite;
	
	public PowerUp(int n,ImageView sprite){
		version=n;
		this.sprite=sprite;
	}
	
	public int getVersion(){
		return version;
	}
	
	public ImageView getSprite(){
		return sprite;
	}

}
